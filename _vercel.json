{
    "version": 3,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "middlewarePath": "_middleware",
            "continue": true
        },
        {
            "handle": "filesystem"
        }
    ],
    "functions": {
        "_middleware.func": {
            "runtime": "edge",
            "entrypoint": "index.js"
        }
    }
}
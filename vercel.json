{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node",
            "config": {
                "distDir": ".vercel/output"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/.vercel/output/$1"
        }
    ]
}